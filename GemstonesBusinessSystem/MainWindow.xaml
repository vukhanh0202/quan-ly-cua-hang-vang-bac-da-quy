<Window x:Class="GemstonesBusinessSystem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
        xmlns:MetroChart="clr-namespace:De.TorstenMandelkow.MetroChart;assembly=De.TorstenMandelkow.MetroChart"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns:local="clr-namespace:GemstonesBusinessSystem"
        mc:Ignorable="d"
        DataContext="{StaticResource MainVM}" 
        x:Name="mainWindow"
        Foreground="#5F7187"
        Title="Diamond" Height="750" Width="1250" FontFamily="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2" WindowStartupLocation="CenterScreen">
    <Window.Resources>

        <Style TargetType="TabItem">
            <Setter Property="Background" Value="#E9ECF2"></Setter>
            <Setter Property="Margin" Value="1"></Setter>
            <Setter Property="Height" Value="30"></Setter>
            <Setter Property="Width" Value="150"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="textBlockMenu">
            <Setter Property="FontSize" Value="20"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="Margin" Value="5 0 0 30"></Setter>
            <Setter Property="FontWeight" Value="Medium"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="textBlockLabel">
            <Setter Property="FontSize" Value="20"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="FontWeight" Value="Medium"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="textBlockTitle">
            <Setter Property="FontSize" Value="20"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="FontWeight" Value="Medium"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2 Medium"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="textBlockContent">
            <Setter Property="FontSize" Value="20"></Setter>
            <Setter Property="Foreground" Value="Black"></Setter>
            <Setter Property="FontWeight" Value="Medium"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="tbl">
            <Setter Property="FontSize" Value="17"></Setter>
            <Setter Property="Margin" Value="20 0 0 0"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>
        <Style TargetType="TextBox" x:Key="txb">
            <Setter Property="FontSize" Value="13"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>
        <Style TargetType="TextBlock" x:Key="tblFooter">
            <Setter Property="FontSize" Value="15"></Setter>
            <Setter Property="FontFamily" Value="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2"></Setter>
        </Style>

        <Style TargetType="StackPanel" x:Key="spMenu">
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="#F2F2F2"></Setter>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Storyboard x:Key="OpenMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="55"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="200"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="CloseMenu">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(FrameworkElement.Width)" Storyboard.TargetName="GridMenu">
                <EasingDoubleKeyFrame KeyTime="0" Value="200"/>
                <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="55"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Window.Resources>
    <Window.Triggers>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonOpenMenu">
            <BeginStoryboard Storyboard="{StaticResource OpenMenu}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="ButtonBase.Click" SourceName="ButtonCloseMenu">
            <BeginStoryboard Storyboard="{StaticResource CloseMenu}"/>
        </EventTrigger>
    </Window.Triggers>
    <Grid Background="#2A323C">
        <Grid x:Name="GridMain" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="55 0 0 0">
            <Grid Height="665">

                <!--Điểm giao dich-->
                <Grid x:Name="DiemBanHang" >
                    <Grid.Style>
                        <Style TargetType="Grid">
                            <Setter Property="Visibility" Value="Hidden"></Setter>
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ChucNang}" Value="9">
                                    <Setter Property="Visibility" Value="Visible"></Setter>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Style>

                    <Grid DataContext="{StaticResource DealVM}">
                        <i:Interaction.Triggers>
                            <i:EventTrigger EventName="LayoutUpdated">
                                <i:InvokeCommandAction Command="{Binding ReloadPageCommand}">
                                </i:InvokeCommandAction>
                            </i:EventTrigger>
                        </i:Interaction.Triggers>
                        <Grid Margin="15 30 20 20"  Background="#323C48">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="5*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <DockPanel Grid.Column="0" Grid.Row="1" Margin="0 0 0 10">
                                <Border VerticalAlignment="Center" DockPanel.Dock="Top" Height="40" Background="#394451" CornerRadius="5"  Margin="0 5 40 10">
                                    <DockPanel>
                                        <Border ToolTip="Tải lại trang" BorderBrush="#394451" BorderThickness="1" Height="40" Width="40" DockPanel.Dock="Right">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="MouseLeftButtonDown">
                                                    <i:InvokeCommandAction Command="{Binding ReloadCommand}">
                                                    </i:InvokeCommandAction>
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                            <Border.Style>
                                                <Style TargetType="Border">
                                                    <Setter Property="Background" Value="#2A323C"></Setter>
                                                    <Style.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="#394451"></Setter>
                                                            <Setter Property="Cursor" Value="Hand"></Setter>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Border.Style>
                                            <materialDesign:PackIcon  ToolTip="Tải lại trang"  VerticalAlignment="Center" Kind="Reload" DockPanel.Dock="Right"
                                                                      Height="30" Foreground="#66D203" Width="30" HorizontalAlignment="Center">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                        <i:InvokeCommandAction Command="{Binding ReloadCommand}">
                                                        </i:InvokeCommandAction>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </materialDesign:PackIcon>
                                        </Border>
                                        <Border  ToolTip="Danh sách khách hàng"  BorderBrush="#394451" BorderThickness="1" Height="40" Width="40" DockPanel.Dock="Right">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="MouseLeftButtonDown">
                                                    <i:InvokeCommandAction Command="{Binding KhachHangCommand}">
                                                    </i:InvokeCommandAction>
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                            <Border.Style>
                                                <Style TargetType="Border">
                                                    <Setter Property="Background" Value="#2A323C"></Setter>
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding GiaoDich}" Value="1">
                                                            <Setter Property="Background" Value="#04A2B3"></Setter>
                                                        </DataTrigger>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="#394451"></Setter>
                                                            <Setter Property="Cursor" Value="Hand"></Setter>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Border.Style>
                                            <materialDesign:PackIcon  ToolTip="Danh sách khách hàng"  VerticalAlignment="Center" Kind="Account"
                                                                      DockPanel.Dock="Right" Height="30" Foreground="White" Width="30" HorizontalAlignment="Center">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                        <i:InvokeCommandAction Command="{Binding KhachHangCommand}">
                                                        </i:InvokeCommandAction>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </materialDesign:PackIcon>
                                        </Border>
                                        <Border ToolTip="Danh sách sản phẩm" BorderThickness="1" Height="40" Width="40" DockPanel.Dock="Right">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="MouseLeftButtonDown">
                                                    <i:InvokeCommandAction Command="{Binding SanPhamCommand}">
                                                    </i:InvokeCommandAction>
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                            <Border.Style>
                                                <Style TargetType="Border">
                                                    <Setter Property="Background" Value="#2A323C"></Setter>
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding GiaoDich}" Value="0">
                                                            <Setter Property="Background" Value="#04A2B3"></Setter>
                                                        </DataTrigger>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="#394451"></Setter>
                                                            <Setter Property="Cursor" Value="Hand"></Setter>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Border.Style>
                                            <materialDesign:PackIcon ToolTip="Danh sách sản phẩm" VerticalAlignment="Center" Kind="FormatListBulleted" DockPanel.Dock="Right"
                                                                     Height="30" Foreground="White" Width="30" HorizontalAlignment="Center">
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                        <i:InvokeCommandAction Command="{Binding SanPhamCommand}">
                                                        </i:InvokeCommandAction>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                            </materialDesign:PackIcon>
                                        </Border>

                                        <materialDesign:PackIcon Margin="10 0" VerticalAlignment="Center" Kind="Search" DockPanel.Dock="Right" Height="40" Foreground="White" Width="40"/>
                                        <TextBox VerticalAlignment="Center" materialDesign:HintAssist.Hint="Tìm kiếm"
                                                 Padding="5 0 0 -10" FontSize="20" Margin="0"
                                                  Text="{Binding TimKiem ,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}" Background="#394451" Foreground="White">
                                            <i:Interaction.Triggers>
                                                <i:EventTrigger EventName="TextChanged">
                                                    <i:InvokeCommandAction Command="{Binding TimKiemCommand}"/>
                                                </i:EventTrigger>
                                            </i:Interaction.Triggers>
                                        </TextBox>
                                    </DockPanel>
                                </Border>
                                <Grid DockPanel.Dock="Top" Background="#323C48" Margin="0 0 20 0">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Collapsed"></Setter>
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding GiaoDich}" Value="1">
                                                    <Setter Property="Visibility" Value="Visible"></Setter>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>
                                    <DockPanel>
                                        <TextBlock DockPanel.Dock="Top" Foreground="#00A4FE" FontSize="25" Padding="10" Text="{Binding TieuDe,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}"></TextBlock>
                                        <DataGrid  Margin="0 0 20 0" GridLinesVisibility="None"
                                          ScrollViewer.VerticalScrollBarVisibility="Visible" DataContext="{StaticResource DealVM}" ScrollViewer.HorizontalScrollBarVisibility="Disabled" SelectionUnit="FullRow"
                                          Background="#323C48" RowDetailsVisibilityMode="VisibleWhenSelected" OverridesDefaultStyle="True"
                                          SelectedItem="{Binding SelectedDetail , UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True,Mode=TwoWay}"
                                          SelectionMode="Extended" ItemsSource="{Binding DSKhachHang}" AutoGenerateColumns="False" IsReadOnly="True">
                                            <DataGrid.Resources>
                                                <Style TargetType="DataGridCell">
                                                    <Setter Property="BorderThickness" Value="0 0 0 1"/>
                                                    <Setter Property="BorderBrush" Value="#566070"/>
                                                    <Setter Property="Height" Value="40"/>
                                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                                    <Setter Property="Padding" Value="20 10 0 0"/>
                                                    <Setter Property="Template">
                                                        <Setter.Value>
                                                            <ControlTemplate TargetType="{x:Type DataGridCell}">
                                                                <Border Padding="{TemplateBinding Padding}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                                                                    <ContentPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                                                                        <ContentPresenter.ContentTemplate>
                                                                            <DataTemplate>
                                                                                <TextBlock Background="Transparent" Name="texte" TextTrimming="CharacterEllipsis" 
                                                                               Height="auto" Width="auto" VerticalAlignment="Center" Text="{Binding Text}"/>
                                                                            </DataTemplate>
                                                                        </ContentPresenter.ContentTemplate>
                                                                    </ContentPresenter>
                                                                </Border>
                                                            </ControlTemplate>
                                                        </Setter.Value>
                                                    </Setter>
                                                    <Style.Triggers>
                                                        <Trigger Property="IsSelected" Value="True">
                                                            <Setter Property="Background" Value="#3A4552"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                                <Style TargetType="{x:Type DataGridColumnHeader}" >
                                                    <Setter Property="Foreground" Value="White" />
                                                    <Setter Property="BorderBrush" Value="#566070"/>
                                                    <Setter Property="BorderThickness" Value="0 0 0 2"/>
                                                    <Setter Property="Height" Value="50" />
                                                    <Setter Property="Width" Value="auto"/>
                                                    <Setter Property="FontSize" Value="15"/>
                                                    <Setter Property="Padding" Value="15 0 0 0"/>
                                                    <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                </Style>
                                            </DataGrid.Resources>
                                            <DataGrid.RowStyle>
                                                <Style TargetType="DataGridRow">
                                                    <Setter Property="Background" Value="#323C48"></Setter>
                                                    <Setter Property="Foreground" Value="#98A4B9"></Setter>
                                                    <Setter Property="BorderBrush" Value="#566070"/>
                                                    <Setter Property="BorderThickness" Value="0"/>
                                                    <Setter Property="Margin" Value="5 0"/>
                                                    <Setter Property="Padding" Value="5 20 0 0"/>
                                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                                    <Style.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="#394451"></Setter>
                                                            <Setter Property="Cursor" Value="Hand"></Setter>
                                                        </Trigger>
                                                        <Trigger Property="IsSelected" Value="True">
                                                            <Setter Property="Background" Value="#323C48"></Setter>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </DataGrid.RowStyle>
                                            <DataGrid.Columns>
                                                <DataGridTextColumn Width="1*" Header="Mã khách hàng" Binding="{Binding MaKhachHang}"  />
                                                <DataGridTextColumn Width="1*" Header="Tên khách hàng"  Binding="{Binding TenKhachHang}"  />
                                                <DataGridTextColumn Width="1*" Header="Số điện thoại"  Binding="{Binding SoDienThoaiKH}"  />
                                                <DataGridTextColumn Width="1*" Header="Địa chỉ"  Binding="{Binding DiaChiKH}"></DataGridTextColumn>
                                                <DataGridTextColumn Width="1*" Header="Tổng bán"  Binding="{Binding TongTienMuaKH}"  />
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </DockPanel>
                                </Grid>
                                <Grid DockPanel.Dock="Top" Background="#323C48" Margin="0 0 40 0">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Collapsed"></Setter>
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding GiaoDich}" Value="0">
                                                    <Setter Property="Visibility" Value="Visible"></Setter>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Grid.Style>
                                    <DockPanel>
                                        <TextBlock DockPanel.Dock="Top" Foreground="#00A4FE" FontSize="25" Padding="10" Text="{Binding TieuDe,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}"></TextBlock>
                                        <ScrollViewer VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Disabled" Padding="0 0 0 0">
                                            <ItemsControl  DataContext="{StaticResource DealVM}"  BorderBrush="#404750" BorderThickness="1 0 1 1" Background="#323C48"
                                                       ItemsSource="{Binding DSSanPham,NotifyOnSourceUpdated=True,UpdateSourceTrigger=PropertyChanged}" >
                                                <ItemsControl.ItemsPanel>
                                                    <ItemsPanelTemplate>
                                                        <WrapPanel  Orientation="Horizontal"></WrapPanel>
                                                    </ItemsPanelTemplate>
                                                </ItemsControl.ItemsPanel>
                                                <ItemsControl.ItemTemplate>
                                                    <DataTemplate>
                                                        <StackPanel Width="160" Height="auto" Margin="10">
                                                            <StackPanel.Style>
                                                                <Style TargetType="StackPanel">
                                                                    <Setter Property="Background" Value="{x:Null}"></Setter>
                                                                    <Style.Triggers>
                                                                        <Trigger Property="IsMouseOver" Value="True">
                                                                            <Setter Property="Background" Value="#394451"></Setter>
                                                                            <Setter Property="Cursor" Value="Hand"></Setter>
                                                                        </Trigger>
                                                                    </Style.Triggers>
                                                                </Style>
                                                            </StackPanel.Style>
                                                            <Image Height="120" Width="120" Source="{Binding HinhAnhSP}" Margin="0 10 0 0" >
                                                                <i:Interaction.Triggers>
                                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}, Path=DataContext.ThemSPVaoHD}"
                                                                                   CommandParameter="{Binding Path=MaSanPham}">
                                                                        </i:InvokeCommandAction>
                                                                    </i:EventTrigger>
                                                                </i:Interaction.Triggers>
                                                            </Image>
                                                            <TextBlock Foreground="White" FontSize="20" Text="{Binding TenSanPham}" FontWeight="Light" Margin="0 5 0 0" TextWrapping="Wrap" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center">
                                                                 <i:Interaction.Triggers>
                                                                     <i:EventTrigger EventName="MouseLeftButtonDown">
                                                                         <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}, Path=DataContext.ThemSPVaoHD}"
                                                                                   CommandParameter="{Binding Path=MaSanPham}">
                                                                         </i:InvokeCommandAction>
                                                                     </i:EventTrigger>
                                                                 </i:Interaction.Triggers>
                                                            </TextBlock>
                                                            <TextBlock Foreground="#C48C46" FontSize="17" Text="{Binding DonGiaBan}" FontWeight="Light" Margin="0 5 0 0" TextWrapping="Wrap" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center">
                                                            </TextBlock>
                                                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                                                <i:Interaction.Triggers>
                                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                                        <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}, Path=DataContext.ThemSPVaoHD}"
                                                                                   CommandParameter="{Binding Path=MaSanPham}">
                                                                        </i:InvokeCommandAction>
                                                                    </i:EventTrigger>
                                                                </i:Interaction.Triggers>
                                                                <TextBlock Foreground="#C48C46" FontSize="13" Text="Số lượng: " FontWeight="Light" Margin="0 -10 0 0" TextWrapping="Wrap" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center">
                                                                </TextBlock>
                                                                <TextBlock Foreground="#C48C46" FontSize="13" Text="{Binding TongSoLuongTon, UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}"
                                                                           FontWeight="Light" Margin="0 -10 0 0" TextWrapping="Wrap" VerticalAlignment="Center" TextAlignment="Center" HorizontalAlignment="Center">
                                                                </TextBlock>
                                                            </StackPanel>
                                                            <TextBlock Foreground="#00A4FE" FontSize="13" Text="Xem chi tiết" FontWeight="Light" Margin="0 5 0 0" TextWrapping="Wrap" VerticalAlignment="Center" TextAlignment="Center">
                                                            <i:Interaction.Triggers>
                                                                <i:EventTrigger EventName="MouseLeftButtonDown">
                                                                     <i:InvokeCommandAction Command="{Binding RelativeSource={RelativeSource AncestorType={x:Type ItemsControl}}, Path=DataContext.DetailCommand}"
                                                                                   CommandParameter="{Binding Path=MaSanPham}">
                                                                    </i:InvokeCommandAction>
                                                                </i:EventTrigger>
                                                            </i:Interaction.Triggers>
                                                                <TextBlock.Style>
                                                                    <Style TargetType="TextBlock" >
                                                                        <Setter Property="Foreground" Value="#0088FF"></Setter>
                                                                        <Style.Triggers>
                                                                            <Trigger Property="IsMouseOver" Value="True">
                                                                                <Setter Property="Foreground" Value="#084E8A"></Setter>
                                                                                <Setter Property="Cursor" Value="Hand"></Setter>
                                                                                <Setter Property="TextDecorations" Value="Underline"></Setter>
                                                                            </Trigger>
                                                                        </Style.Triggers>
                                                                    </Style>
                                                                </TextBlock.Style>
                                                            </TextBlock>
                                                        </StackPanel>
                                                    </DataTemplate>
                                                </ItemsControl.ItemTemplate>
                                            </ItemsControl>
                                        </ScrollViewer>
                                    </DockPanel>
                                </Grid>

                            </DockPanel>
                            <Border VerticalAlignment="Top" Grid.RowSpan="2" Grid.Column="1" BorderBrush="#00A4FE" BorderThickness="1 0 1 1" Margin="0 0 0 0">
                                <DockPanel >
                                    <Grid DockPanel.Dock="Top" Background="#00A4FE" Margin="0 0 -1 0">
                                        <StackPanel Orientation="Horizontal">
                                            <materialDesign:PackIcon Margin="15 0 0 0" Kind="Cash" Height="35" Width="35" Foreground="White"/>
                                            <TextBlock Padding="10 0 0 0" VerticalAlignment="Center" Foreground="White"  FontSize="22" Text="Thanh toán"></TextBlock>
                                        </StackPanel>
                                    </Grid>
                                    <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
                                        <Image Height="100" Width="100" Source="Images/NoName.png" Margin="10"></Image>
                                        <StackPanel Orientation="Vertical" Margin="0 15 0 0">
                                            <TextBlock Text="Nhân viên" Foreground="White" FontSize="22"></TextBlock>
                                            <TextBlock Text="Nguyễn Vũ Khánh" Foreground="White" FontSize="17"></TextBlock>
                                            <StackPanel Orientation="Horizontal" Margin="0 5 0 0">
                                                <TextBlock Text="Ngày tạo:   " Foreground="White" FontSize="14"></TextBlock>
                                                <TextBlock Text="{Binding NgayTao, UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}" Foreground="White" FontSize="14"></TextBlock>
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                    <DockPanel LastChildFill="False">
                                        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
                                            <TextBlock Text="Thành viên" FontSize="20" Margin="5 5 5 0" Foreground="White" VerticalAlignment="Center" ></TextBlock>
                                            <ComboBox FontSize="17" Background="#394451" Padding="5 " Height="auto"  Foreground="White" Width="180" materialDesign:HintAssist.Hint="Chọn thành viên"
                                                                  IsEditable="True" ItemsSource="{Binding DSKhachHangDB}" DisplayMemberPath="TenKhachHang" 
                                                      SelectedItem="{Binding KHDaChon,UpdateSourceTrigger=PropertyChanged,NotifyOnSourceUpdated=True}">
                                            </ComboBox>
                                            <Grid>
                                                <i:Interaction.Triggers>
                                                    <i:EventTrigger EventName="MouseLeftButtonDown">
                                                        <i:InvokeCommandAction Command="{Binding ThemKHCommand}">
                                                        </i:InvokeCommandAction>
                                                    </i:EventTrigger>
                                                </i:Interaction.Triggers>
                                                <materialDesign:PackIcon ToolTip="Thêm thành viên mới" Margin="5 0 0 0" Opacity="0.85" Kind="PlusCircle"
                                                                             Foreground="#F8F9FA" VerticalAlignment="Center" Height="35" Width="35">
                                                    <materialDesign:PackIcon.Style>
                                                        <Style TargetType="materialDesign:PackIcon">
                                                            <Style.Triggers>
                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                    <Setter Property="Cursor" Value="Hand"></Setter>
                                                                </Trigger>
                                                            </Style.Triggers>
                                                        </Style>
                                                    </materialDesign:PackIcon.Style>
                                                </materialDesign:PackIcon>
                                            </Grid>
                                        </StackPanel>
                                        <DockPanel  DockPanel.Dock="Top" LastChildFill="True">
                                            <TextBlock Margin="15 5 0 0" Text="Tổng tiền" FontSize="22" FontWeight="Bold"  Foreground="#66D203" VerticalAlignment="Center" FontFamily="/GemstonesBusinessSystem;component/fonts/#Baloo Da 2 Medium"></TextBlock>
                                            <TextBlock TextAlignment="Right" Padding="5 0" Background="#394451" DockPanel.Dock="Right" Margin="10 5 15 0" FontSize="22" Text="{Binding TongTien, UpdateSourceTrigger=PropertyChanged, NotifyOnSourceUpdated=True}" Foreground="#66D203" VerticalAlignment="Center">
                                            </TextBlock>
                                        </DockPanel>
                                        <Border Height="250" Margin="10 0 10 10" DockPanel.Dock="Top" BorderThickness="0 1 0 0" BorderBrush="#394451">
                                            <DataGrid Margin="0" GridLinesVisibility="None" 
                                          ScrollViewer.VerticalScrollBarVisibility="Visible" ScrollViewer.HorizontalScrollBarVisibility="Disabled" SelectionUnit="FullRow"
                                          Background="#323C48"  DataContext="{StaticResource DealVM}" CanUserAddRows="False"
